{
  "compilerOptions": {
    "declaration": true,
    "sourceMap": true,
    "lib": ["ESNext"],
    "module": "CommonJS",
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "strict": true,
    "target": "ESNext",
    "skipLibCheck": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "resolveJsonModule": true,
    "outDir": "dist"
  },
  "exclude": [],
  "include": ["src/**/*"]
}
