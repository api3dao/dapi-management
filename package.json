{
  "name": "@api3/dapi-management",
  "version": "0.1.0",
  "license": "MIT",
  "description": "API3 dAPI management",
  "repository": {
    "type": "git",
    "url": "https://github.com/api3dao/dapi-management.git"
  },
  "private": false,
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "scripts": {
    "build": "hardhat compile",
    "lint": "yarn run format:check && yarn run lint:eslint && yarn run lint:solhint",
    "lint:solhint": "solhint ./contracts/**/*.sol",
    "lint:eslint": "eslint . --ext .js,.ts,.tsx",
    "format:check": "prettier --check \"./**/*.{js,ts,tsx,css,md,json,sol}\"",
    "format": "prettier --write \"./**/*.{js,ts,tsx,css,md,json,sol}\"",
    "deploy": "hardhat deploy --export-all deployments/references.json",
    "deploy-only": "hardhat deploy --tags deploy-all --export-all deployments/references.json",
    "node": "hardhat node --no-deploy",
    "node-deploy": "hardhat node",
    "node-deploy-only": "hardhat node --tags deploy-all",
    "test": "hardhat test && yarn frontend:test",
    "test:coverage": "hardhat coverage",
    "test:gas": "REPORT_GAS=TRUE hardhat test",
    "write-env-example": "node scripts/write-env-example.js",
    "sign": "node scripts/signMerkleTree.js",
    "sync-dapi-fallback-values": "node scripts/sync-dapi-fallback-values.js",
    "verify-metadata": "node scripts/verify-metadata.js",
    "frontend:dev": "cd frontend && next dev",
    "frontend:build": "cd frontend && next build",
    "frontend:start": "cd frontend && next start",
    "frontend:type-check": "yarn tsc --project ./frontend/tsconfig.json",
    "frontend:test": "jest --config frontend/jest.config.js",
    "frontend:add-component": "npx shadcn-ui@latest add --cwd frontend"
  },
  "devDependencies": {
    "@api3/chains": "^3.5.1",
    "@jest/globals": "^29.7.0",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^2.0.2",
    "@nomiclabs/hardhat-ethers": "^2.0.3",
    "@nomiclabs/hardhat-etherscan": "^3.0.0",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.2",
    "@types/chai": "^4.2.0",
    "@types/jest": "^29.5.8",
    "@types/lodash": "^4.14.200",
    "@types/mocha": ">=9.1.0",
    "@types/node": "^20.8.7",
    "@types/react": "^18.2.29",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "autoprefixer": "^10.4.16",
    "chai": "^4.2.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.44.0",
    "eslint-config-next": "13.5.6",
    "eslint-config-prettier": "^9.0.0",
    "hardhat": "^2.16.1",
    "hardhat-deploy": "^0.11.43",
    "hardhat-gas-reporter": "^1.0.8",
    "jest": "^29.7.0",
    "postcss": "^8.4.31",
    "prettier": "^2.8.8",
    "prettier-plugin-solidity": "^1.1.3",
    "prettier-plugin-tailwindcss": "0.4.1",
    "solhint": "^3.4.1",
    "solidity-coverage": "^0.8.1",
    "tailwindcss": "^3.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": ">=8.0.0",
    "typechain": "^8.1.0",
    "typescript": ">=4.5.0"
  },
  "dependencies": {
    "@api3/airnode-protocol-v1": "^2.10.0",
    "@api3/promise-utils": "^0.4.0",
    "@nodary/utilities": "^0.7.1",
    "@openzeppelin/contracts": "4.9.3",
    "@openzeppelin/merkle-tree": "^1.0.5",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-tooltip": "^1.0.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "diff2html": "^3.4.45",
    "ethers": "^5.7.2",
    "lodash": "^4.17.21",
    "lucide-react": "^0.288.0",
    "next": "13.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  }
}
